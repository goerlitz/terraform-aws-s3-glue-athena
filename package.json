    "@aws-sdk/lib-storage": "^3.75.0",
    "athena-express": "^7.1.5",
    "aws-sdk": "^2.1108.0",
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/node": "^17.0.23",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
