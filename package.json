{
  "scripts": {
    "compile": "tsc",
    "build": "tsc && cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.ts",
    "deploy": "npm run build && npm run apply-lambdas",
    "plan-lambdas": "terraform -chdir=terraform/stage/services/lambdas plan",
